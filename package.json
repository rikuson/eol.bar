{
  "name": "eol.bar",
  "version": "0.0.1-0",
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "scripts": {
    "fetch": "node ./bin/fetch",
    "serve": "node ./bin/www",
    "serve:watch": "nodemon ./bin/www",
    "dev": "run-p test:watch serve:watch",
    "prestart": "mkdir -p data && curl -o data/all.json https://endoflife.date/api/all.json",
    "start": "run-p fetch serve",
    "test": "mocha",
    "test:watch": "mocha --watch"
  },
  "dependencies": {
    "chai": "^4.3.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "fast-xml-parser": "^4.0.0-beta.8",
    "mocha": "^9.1.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
