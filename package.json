{
  "name": "eol.bar",
  "version": "0.0.1-3",
  "engines": {
    "node": "16.x"
  },
  "private": true,
  "scripts": {
    "fetch": "node ./bin/fetch",
    "serve": "node ./bin/www",
    "serve:watch": "nodemon ./bin/www",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "dev": "NODE_ENV=development run-p test:watch serve:watch docker:up",
    "prestart": "curl -o data.json https://endoflife.date/api/all.json",
    "start": "run-p fetch serve",
    "test": "mocha",
    "test:watch": "mocha --watch"
  },
  "dependencies": {
    "chai": "^4.3.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv-flow": "^3.2.0",
    "express": "~4.16.1",
    "fast-xml-parser": "^4.0.0-beta.8",
    "mocha": "^9.1.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.2",
    "redis": "^4.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
